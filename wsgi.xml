<uwsgi>
 <socket>127.0.0.1:9000</socket>
 <listen>200</listen>
 <master>true</master>
 <pidfile>/var/run/nginx.pid</pidfile>
 <processes>8</processes>
 <pythonpath>/home/hankin/MySite/</pythonpath>
 <module>wsgi</module>
 <profiler>true</profiler>
